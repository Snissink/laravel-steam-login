{
    "name": "kanalumaddela/laravel-steam-login",
    "description": "Steam Login/Auth package for Laravel/Lumen 5.5+ / 6.x+ / 7.x+ / 8.x+",
    "keywords": [
        "laravel",
        "authentication",
        "auth",
        "login",
        "steam",
        "openid",
        "lumen",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sam",
            "email": "git@maddela.org",
            "homepage": "https://www.maddela.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "xpaw/steamid": "^2.0"
    },
    "suggest": {
        "kanalumaddela/laravel-steam-socialite": "Driver for Laravel Socialite (community)"
    },
    "autoload": {
        "psr-4": {
            "kanalumaddela\\LaravelSteamLogin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kanalumaddela\\LaravelSteamLogin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "kanalumaddela\\LaravelSteamLogin\\SteamLoginServiceProvider"
            ],
            "aliases": {
                "SteamLogin": "kanalumaddela\\LaravelSteamLogin\\Facades\\SteamLogin"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.0"
    }
}
